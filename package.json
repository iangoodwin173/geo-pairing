  "bugs": {
    "url": "https://github.com/iangoodwin173/geo-pairing/issues"
  },
  "devDependencies": {
    "concurrently": "^5.3.0"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "express-graphql": "^0.12.0"
  }
}
